<h1>Create New Account</h1>


  <%= form_for @traveler do |f| %>
    <%= f.label :Name %>
    <%= f.text_field :name %>
    <%= f.label :Username %>
    <%= f.text_field :username %>
    <%= f.label :Email %>
    <%= f.text_field :email %>
    <%= f.label :Password %>
    <%= f.text_field :password %>
    <%= f.label :Confirm_Password %>
    <%= f.text_field :password_confirmation %>
    <%= f.submit %>
    <% end %>
